{
    "name": "innmind/html",
    "type": "library",
    "description": "Library to abstract html manipulation",
    "keywords": ["html"],
    "homepage": "http://github.com/Innmind/Html",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Html/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/xml": "~8.0",
        "innmind/filesystem": "~8.1",
        "innmind/immutable": "~5.16",
        "innmind/validation": "~2.0",
        "symfony/dom-crawler": ">=6.3 <7.0.7",
        "innmind/url": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Html\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Html\\": "tests/"
        }
    },
    "require-dev": {
        "innmind/black-box": "^6.4.1",
        "innmind/static-analysis": "^1.2.1",
        "innmind/coding-standard": "~2.0"
    }
}
