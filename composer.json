{
    "name": "innmind/html",
    "type": "library",
    "description": "Library to abstract html manipulation",
    "keywords": ["html"],
    "homepage": "http://github.com/Innmind/Html",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Html/issues"
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/xml": "~6.0",
        "symfony/dom-crawler": "^4.2|^5.0",
        "innmind/url": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Html\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Html\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/coding-standard": "^1.1"
    }
}
