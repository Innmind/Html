{
    "name": "innmind/html",
    "type": "library",
    "description": "Library to abstract html manipulation",
    "keywords": ["html"],
    "homepage": "http://github.com/Innmind/Html",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Html/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/xml": "~7.1",
        "symfony/dom-crawler": "~6.0",
        "innmind/url": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Html\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Html\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.22",
        "innmind/coding-standard": "~2.0"
    }
}
